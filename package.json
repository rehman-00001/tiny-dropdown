{
  "name": "tiny-dropdown",
  "version": "0.1.2",
  "description": "A simple, customizable, light-weight dropdown component built with love using React",
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "2.1.8"
  },
  "scripts": {
    "start": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o .out",
    "build-component": "webpack --mode production --config ./webpack/config.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@storybook/addon-actions": "^5.0.6",
    "@storybook/addon-links": "^5.0.6",
    "@storybook/addons": "^5.0.6",
    "@storybook/react": "^5.0.6",
    "@storybook/storybook-deployer": "^2.8.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^2.1.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rehman-00001/tiny-dropdown.git"
  },
  "keywords": [
    "react",
    "dropdown",
    "javascript",
    "component",
    "storybook"
  ],
  "author": "Abdul Rehman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rehman-00001/tiny-dropdown/issues"
  },
  "homepage": "https://github.com/rehman-00001/tiny-dropdown#readme",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-class-properties"
    ]
  }
}
